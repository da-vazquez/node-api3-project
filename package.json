{
    "name": "node-api3-project",
    "version": "1.0.0",
    "scripts": {
        "start": "node index.js",
        "server": "nodemon --require dotenv/config index.js",
        "resetdb": "knex migrate:rollback && knex migrate:latest && knex seed:run",
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "heroku-postbuild": "npm run build"
    },
    "engines": {
        "node": "14.15.4",
        "npm": "6.14.11"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LambdaSchool/node-api3-project.git"
    },
    "dependencies": {
        "express": "^4.17.1",
        "knex": "^0.95.4",
        "serve": "^11.3.2",
        "sqlite3": "^5.0.2"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.23.0",
        "jest": "^26.6.3",
        "knex-cleaner": "^1.3.1",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3"
    }
}
